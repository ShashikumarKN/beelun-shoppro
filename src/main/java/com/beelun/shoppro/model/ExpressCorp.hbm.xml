<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.beelun.shoppro.model.ExpressCorp" table="shoppro_expresscorp" lazy="false">
        <id name="id" column="id" unsaved-value="null">
            <generator class="increment"/>
        </id> 
        <property name="shortName" column="shortName" not-null="true"/>
        <property name="fullName" column="fullName" />
        <property name="enabled" column="enabled" not-null="true"/>
        <property name="webSite" column="webSite" />
    </class>
</hibernate-mapping>
