<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 1.0//EN"
    "http://tuckey.org/res/dtd/urlrewrite1.dtd">

<!-- TODO: add .html to url which don't have extension to enable better seo -->

<urlrewrite>
	<!-- !!! Below rule must be put ahead of /$ -->
    <rule>
        <from>/admin/$</from>
        <to type="forward">index.html</to>
    </rule>
    <rule>
        <from>/$</from>
        <to type="forward">index.html</to>
    </rule>
